emp_skill = input("Enter your skills:")
if emp_skill in Devops:
    print("This {} skills is in Devops group".format(Devops))
elif(emp_skill in Dev):
    print("This {} skill is in Dev Team".format(Dev))
elif(emp_skill in Emp_Info1):
    print("This {} skill is in E1 Team".format(Emp_Info1))

elif(emp_skill in Emp_Info2):
    print(f"This {Emp_Info2} skill is in E2 Team")

elif(emp_skill in Emp_Info3):
    print("This {} skill is in E3 Team".format(Emp_Info3))
